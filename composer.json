{
    "name": "hectorj/safe-php-psalm-plugin",
    "description": "vimeo/psalm plugin for thecodingmachine/safe",
    "type": "psalm-plugin",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-simplexml": "*",
        "vimeo/psalm": "^4.0",
        "thecodingmachine/safe": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "hectorj"
        }
    ],
    "extra": {
        "psalm": {
            "pluginClass": "HectorJ\\SafePHPPsalmPlugin\\Plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "HectorJ\\SafePHPPsalmPlugin\\": ["./src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HectorJ\\SafePHPPsalmPlugin\\Tests\\": ["./tests"]
        }
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^9.0"
    }
}
